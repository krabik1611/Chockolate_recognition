<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Мусульманский_зелёный</item>
        <item name="colorPrimaryDark">@color/Мусульманский_зелёный</item>
        <item name="colorAccent">@color/Мусульманский_зелёный</item>

    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="switchabc" parent="Theme.AppCompat.Light">
        <item name="color">@color/Мусульманский_зелёный</item>
    </style>
    <style name="fabBlack" parent="Widget.Design.FloatingActionButton">
        <item name="backgroundTint">@android:color/black</item>
    </style>



</resources>